<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moe.finalproject">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >

        <activity android:name="com.moe.finalproject.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.moe.finalproject.LivingRoom"></activity>
        <activity android:name="com.moe.finalproject.TVDetails" />
        <activity android:name="com.moe.finalproject.LightDetails" />
        <activity android:name="com.moe.finalproject.BlindingDetails" />
        <activity android:name="com.mohibhero.finalproject.HouseSettings"></activity>
        <activity android:name="com.mohibhero.finalproject.GarageDoor"></activity>
        <activity android:name="com.mohibhero.finalproject.Temperature"></activity>
        <activity android:name="com.mohibhero.finalproject.WeatherForecast"></activity>
        <activity android:name="com.emad.finalproject.Kitchen" />
        <activity android:name="com.emad.finalproject.KitchenFridge" />
        <activity android:name="com.emad.finalproject.KitchenMicrowave"></activity>
    </application>

</manifest>